{
    "parser": "@typescript-eslint/parser", 
    "plugins": ["@typescript-eslint", "prettier"],
    "extends": ["eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"],
    "rules": {
        "prettier/prettier": [
            "error", 
            {
                "singleQuote": true,
                "semi": true,
                "tabWidth": true,
                "trailingComma": "off",
                "printWidth": 100
             }
        ],
        "@typescript-eslint/no-explicit-any": "error"
    }
    
}